{
  "name": "@vitejs/test-monorepo-vite-condition-exports",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "debug": "node --inspect-brk ../../packages/vite/bin/vite",
    "preview": "vite preview"
  },
  "#exports": "Fake paths on purpose so that they don't resolve!",
  "exports": {
    ".": {
      "vite": "./src/index.ts",
      "module": "./build/index.js"
    },
    "./*": {
      "vite": [
        "./src/*.ts",
        "./src/*.tsx"
      ],
      "module": "./build/*.js"
    }
  }
}
